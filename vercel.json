{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "SAP-Customer.html",
      "headers": {
        "Content-Type": "text/html"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
